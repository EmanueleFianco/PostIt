<li class="nota">

	<div class="id">{{id}}</div>
    <div class="editnota">
     	<img id="edit_button" src="JS/view/Image/editnota.png"></img>
     </div>
     
	<div class="TitoloNota" contenteditable="true">{{titolo}}</div>
		
	<div class="colorPicker" >
        	<a class="color"><div class="colorInner"></div></a>
        	<div class="track"></div>
        	<ul class="dropdown"><li></li></ul>
        	<input type="hidden" class="colorInput"/>
    </div>
	
	<div class="TestoNota">
		<textarea class="redactor">{{testo}}</textarea>
	</div>
	
</li>